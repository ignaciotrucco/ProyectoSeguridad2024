@{
    ViewData["Title"] = "Provincias";
}

<div class="text-center">
    <h1 class="display-4">Provincias</h1>
</div>

<div class="row">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <input id="ProvinciaID" type="hidden">
                <input placeholder="Nombre de la provincia" id="NombreProvincia" type="text" class="validate">
                <label for="NombreProvincia">Nombre</label>
            </div>
            <div class="input-field col s6">
                <button type="button" class="waves-effect waves-light btn">Guardar</button>
            </div>
        </div>
    </form>
</div>

<table>
        <thead>
          <tr>
              <th>Nombre</th>
              <th>Editar</th>
              <th>Eliminar</th>
          </tr>
        </thead>

        <tbody id=""></tbody>
      </table>